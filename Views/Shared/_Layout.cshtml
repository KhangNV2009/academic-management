<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AcademicManagement</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1"
          crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link href="~/css/Login.css" rel="stylesheet" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="shadow-sm">
            <div class="greenwich-bag">
                <img class="museum-icon" src="~/museum.png" />
                <div class="greenwich-text">
                    <h4 class="color-primary">UNIVERSITY OF <br />GREENWICH</h4>
                    <p class="small-text color-primary">ALLIANCE WITH FPT EDUCATION</p>
                </div>
            </div>
        </nav>
    </header>
    <main class="min-height container">
        @RenderBody()
    </main>
    <footer class="border-top footer text-muted bg-light">
        <div class="container">
            &copy; 2020 - Academic management system made by Power team- <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            <div class="row">
                <div class="col">
                    <h3>Team members</h3>
                    <div>Nguyễn Vĩ Khang - GCS18028 - khangnvgcs18028@fpt.edu.vn</div>
                    <div>Nguyễn Đức Khang - GCS18830 - khangndgcs18830@fpt.edu.vn</div>
                    <div>Đinh Trọng Tâm - GCS18278 - tamdt18278@fpt.edu.vn</div>
                </div>
                <div class="col d-flex justify-content-end">
                    <img src="~/img/logoGreenwich.png" height="150" />
                </div>
            </div>
        </div>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>
    @await RenderSectionAsync("Scripts", required: false)
    @await RenderSectionAsync("Style", required: false)
</body>
</html>
